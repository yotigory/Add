<![CDATA[  //hover bgcolor
	$(".mod_article_main_product_entry").hover(function() {
   $(this).stop().animate({ backgroundColor: "#EEE"}, 400);//ON
    },function() {
   $(this).stop().animate({ backgroundColor: "#fff" }, 400);//OFF
    });]]>
